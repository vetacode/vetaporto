{
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA": "true"
    }
  },
  "headers": [
    {
      "source": "/icon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
